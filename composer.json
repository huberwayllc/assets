{
    "name": "huberwaycms/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "huberwaycms",
        "huberwaycms cms",
        "huberwaycms platform",
        "huberwaycms assets"
    ],
    "homepage": "https://huberway.com",
    "authors": [
        {
            "name": "Sang Nguyen",
            "email": "sangnguyenplus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HuberwayCMS\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuberwayCMS\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuberwayCMS\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "HuberwayCMS\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
